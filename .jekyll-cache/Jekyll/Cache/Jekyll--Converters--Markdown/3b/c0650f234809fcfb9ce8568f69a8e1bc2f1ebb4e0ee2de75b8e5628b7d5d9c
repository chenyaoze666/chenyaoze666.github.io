<div class="highlighter-rouge"><div class="highlight"><pre class="highlight"><code>  function TX(content,row,col,hollow){
    if(row&lt;3){
      console.log("è¯·è¾“å…¥è¡Œæ•°å°äº3");
      return ;
    }
    var tmp = "";
    if(!hollow){
      console.log("æ‰“å°æ¢¯å½¢");
      for(var i=0;i&lt;row;i++){
         tmp = "";
         for( var j =0; j&lt;row-1-i; j++){
            tmp+=" ";
         }
         for( var k =0; k&lt;2*i+4; k++){
            tmp+=content;
         }
         console.log(tmp);
      }
    }else{
      console.log("æ‰“å°ç©ºå¿ƒæ¢¯å½¢");
      for(var i=0;i&lt;row;i++){
        tmp = "";
        for( var j =0; j&lt;row-1-i; j++){
            tmp+=" ";
        }
        if(i==0 || i==row-1){
          for( var k =0; k&lt;2*i+4; k++){
            tmp+=content;
          }
        }else{
          tmp+=content;
          for( var k =0; k&lt;2*i+2; k++){
             tmp+=" ";
          }
          tmp+=content;
        }
        console.log(tmp);
      }
    }
  }


