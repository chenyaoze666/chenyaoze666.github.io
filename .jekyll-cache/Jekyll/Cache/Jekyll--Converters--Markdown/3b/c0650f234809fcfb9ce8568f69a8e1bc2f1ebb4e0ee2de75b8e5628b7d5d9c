function SJX(content,row,col,hollow){
    if(row!=col || row&lt;4){
      console.log("è¾“å…¥è¡Œæ•°é”™è¯¯æˆ–è¡Œæ•°&lt;4æˆ–è¡Œåˆ—ä¸ç­‰");
      return;
    }
    var tmp ="";
    if(!hollow){
        console.log("æ‰“å°ä¸‰è§’å½¢");
        for(var i =0; i&lt;row; i++)
        {
          tmp = "";
          for(var j=0; j&lt;=i; j++){
              tmp+=content;
              tmp+=" ";
          }
          console.log(tmp);
        }
    }else{
        console.log("æ‰“å°ç©ºå¿ƒä¸‰è§’å½¢");
        for(var i=0; i&lt;row; i++)
        {
          tmp = "";
        if(i==0 || i==1 || i==row-1){
            for(var j=0; j&lt;=i; j++){
              tmp+=content;
              tmp+=" ";
            }
        }else{
              tmp+=content;
              tmp+=" ";
          for(var j=0; j&lt;(i-1)*2; j++){
             tmp+=" ";
          }
              tmp+=content;
              tmp+=" ";
        }
          console.log(tmp);
       }
    }
    
}

  function LX(content,row,col,hollow){
    var temp ="";
    var tmp;
    var rowtmp=(row-1)/2
    if(!hollow){
      console.log("æ‰“å°è±å½¢");
      for(var i=-rowtmp; i&lt;=rowtmp; i++){
        temp="";
        tmp = Math.abs(i);
        for(var j = 0; j&lt;tmp;j++){
          temp+=" ";
        }
        for(var k=0; k&lt;row-2*tmp; k++){
          temp+=content;
        }
        console.log(temp);
      }
    }else{
      console.log("æ‰“å°ç©ºå¿ƒè±å½¢");
      for(var i=-rowtmp; i&lt;=rowtmp; i++){
        temp="";
        tmp = Math.abs(i);
        for(var j = 0; j&lt;tmp;j++){
          temp+=" ";
        }
        if(tmp==rowtmp || tmp ==rowtmp-1){
          for(var k=0; k&lt;rowtmp-tmp+1; k++){
            temp+=content;
            temp+=" ";
          }
        }else{
            temp+=content;
            temp+=" ";
          for(var k=0; k&lt;row-2*tmp-3; k++){
            temp+=" ";
          }
            temp+=content;
            temp+=" ";
        }
        console.log(temp);
      }
    }
  }
