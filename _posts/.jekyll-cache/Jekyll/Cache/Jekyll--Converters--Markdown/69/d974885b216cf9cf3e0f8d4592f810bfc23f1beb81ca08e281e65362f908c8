int global=0;
    
int rect(int a, int b)
{
    static int count=0;
    count++;
    global++;
    int s=a*b;
    return s;
}
    
int quadrate(int a)
{
    static int count =0;
    count++;
    global++;
    int s=rect(a,a);
        return s;
}
    
int main()
{
    int a=3;
    int b=4;
    int *pa=&amp;a;
    int *pb=&amp;b;
    int *pglobal=&amp;global;
    int (*pquadrate)(int a)=&amp;quadrate;
