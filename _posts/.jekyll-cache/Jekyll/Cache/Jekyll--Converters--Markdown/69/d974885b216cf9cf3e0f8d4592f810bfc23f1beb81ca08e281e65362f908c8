{
    int tmp =*a;
    *a=*b;
    *b=tmp;
}
  
  
int main()
{
    int a=5;
    int b=3;
      
    change(&amp;a,&amp;b);
      
    printf("num a=%d\n num b =%d\n",a,b);    
    return 0;
}
</code></pre></div>    </div>
