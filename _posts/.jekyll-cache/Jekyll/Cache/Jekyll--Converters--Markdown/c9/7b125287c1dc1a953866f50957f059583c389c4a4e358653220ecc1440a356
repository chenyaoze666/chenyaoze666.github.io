<div class="highlighter-rouge"><div class="highlight"><pre class="highlight"><code>#include &lt;stdio.h&gt;

int main(){

    int a[] ={3,4,17,8,31,2,9,15},n,i,j;
    int t;

    n=sizeof(a)/sizeof(int);

    for(i=0;i&lt;n-1;i++){
        for(j=0;j&lt;n-1-i;j++){
            if(a[j]&gt;a[j+1]){
                t=a[j];
                a[j]=a[j+1];
                a[j+1]=t;
            }
        }
    }

    for(i=0;i&lt;n;i++)
    	printf("%d ",a[i]);
    return 0;
}
